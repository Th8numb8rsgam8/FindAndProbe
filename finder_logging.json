{
	"version": 1,
	"formatters": 
	{
		"console_fmt": 
		{
			"()": "custom_logging.ConsoleLogging",
			"fmt": "[%(asctime)s] | %(name)s | %(levelname)s | %(message)s",
			"datefmt": "%m/%d/%Y %I:%M:%S %p",
			"msecfmt": ""
		},
		"finder_fmt":
		{
			"()":"custom_logging.FinderLogging",
			"fmt": "[%(asctime)s] | %(processName)s | %(levelname)s | %(message)s",
			"datefmt": "%m/%d/%Y %I:%M:%S %p",
			"msecfmt": ""
		}
	},
	"handlers": 
	{
		"console": 
		{
			"level": "INFO",
			"class": "logging.StreamHandler",
			"formatter": "console_fmt",
			"stream": "ext://sys.stdout"
		},
		"finder_handler":
		{
			"class": "logging.FileHandler",
			"level": "WARNING",
			"formatter": "finder_fmt",
			"filename": "finder.log"
		}
	},
	"loggers": 
	{
		"finderlog": 
		{
			"level": "INFO",
			"handlers": ["finder_handler"],
			"propagate": true,
			"qualname": "finderlog"
		}
	},
	"root": 
	{
		"level": "INFO",
		"handlers": ["console"]
	}
}
