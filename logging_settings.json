{
	"version": 1,
	"formatters": 
	{
		"console_fmt": 
		{
			"()": "custom_logging.ConsoleLogging",
			"fmt": "[%(asctime)s] | %(processName)s | %(levelname)s | %(message)s",
			"datefmt": "%m/%d/%Y %I:%M:%S %p",
			"msecfmt": ""
		},
		"finder_fmt":
		{
			"()":"custom_logging.FinderLogging",
			"fmt": "%(relativeCreated)-25s %(message)s",
			"datefmt": "%m/%d/%Y %I:%M:%S %p",
			"msecfmt": ""
		}
	},
	"filters":
	{
		"console_filter":
		{
			"()": "custom_logging.ConsoleFilter",
			"name": "console_filter"
		},
		"finder_filter":
		{
			"()": "custom_logging.FinderFilter",
			"name": "finder_filter"
		}
	},
	"handlers": 
	{
		"console": 
		{
			"level": "INFO",
			"class": "logging.StreamHandler",
			"filters": ["console_filter"],
			"formatter": "console_fmt",
			"stream": "ext://sys.stdout"
		},
		"finder_handler":
		{
			"level": "INFO",
			"class": "logging.FileHandler",
			"filters": ["finder_filter"],
			"formatter": "finder_fmt",
			"filename": "finder.log"
		}
	},
	"root": 
	{
		"level": "INFO",
		"handlers": ["console", "finder_handler"]
	}
}
