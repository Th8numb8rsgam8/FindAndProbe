{
	"version": 1,
	"formatters": 
	{
		"console_fmt": 
		{
			"()": "logs.custom_logging.ConsoleLogging",
			"fmt": "[%(asctime)s] | %(processName)s | %(process)d | %(levelname)s | %(message)s",
			"datefmt": "%m/%d/%Y %I:%M:%S %p",
			"msecfmt": "",
			"defaults": 
			{
				"payload": "None",
				"resposne": "None"
			}
		},
		"finder_fmt":
		{
			"()": "logs.custom_logging.FinderLogging",
			"fmt": "%(relativeCreated)-25s %(message)s",
			"datefmt": "%m/%d/%Y %I:%M:%S %p",
			"msecfmt": "",
			"defaults": 
			{
				"payload": "None",
				"response": "None"
			}
		},
		"probe_fmt":
		{
			"()": "logs.custom_logging.ProbeLogging",
			"fmt": "%(relativeCreated)-25s %(message)s | %(payload)s | %(response)s",
			"datefmt": "%m/%d/%Y %I:%M:%S %p",
			"msecfmt": "",
			"defaults": 
			{
				"payload": "None",
				"response": "None"
			}
		}
	},
	"filters":
	{
		"console_filter":
		{
			"()": "logs.custom_logging.ConsoleFilter",
			"name": "console_filter"
		},
		"finder_filter":
		{
			"()": "logs.custom_logging.FinderFilter",
			"name": "finder_filter"
		},
		"probe_filter":
		{
			"()": "logs.custom_logging.ProbeFilter",
			"name": "probe_filter"
		}
	},
	"handlers": 
	{
		"console": 
		{
			"level": "INFO",
			"class": "logging.StreamHandler",
			"filters": ["console_filter"],
			"formatter": "console_fmt",
			"stream": "ext://sys.stdout"
		},
		"finder_handler":
		{
			"level": "INFO",
			"class": "logging.FileHandler",
			"filters": ["finder_filter"],
			"formatter": "finder_fmt",
			"filename": "finder.log",
			"mode": "w"
		},
		"probe_handler":
		{
			"level": "INFO",
			"class": "logging.FileHandler",
			"filters": ["probe_filter"],
			"formatter": "probe_fmt",
			"filename": "probe.log",
			"mode": "w"
		}
	},
	"root": 
	{
		"level": "INFO",
		"handlers": ["console", "finder_handler", "probe_handler"]
	}
}
